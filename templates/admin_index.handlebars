<div class="row-fluid">
  <div class="span12">
    <div class="content-box">
      <div class="content-box-header">
        <i class="icon-bar-chart"></i> Crash Reports
      </div>

      <div class="row-fluid">
        <div class="span12">
          <ul>
          {{#each crashes }}
            <li><strong>{{this.applicationname}}</strong> {{this.platform}}</li>
          {{/each}}
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
